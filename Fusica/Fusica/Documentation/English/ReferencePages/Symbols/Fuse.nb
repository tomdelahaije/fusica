 Cell[BoxData[
  SubscriptBox[
   StyleBox["symbol", "TI"], 
   StyleBox["2", "TR"]]], "InlineFormula",ExpressionUUID->
  "cfec64be-bf98-4f1a-a1de-3a4a8e1ec590"],
