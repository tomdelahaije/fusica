  ButtonBox["Fuse",
   BaseStyle->"Link",
   ButtonData->"paclet:Fusica/ref/Fuse"]], "InlineFormula",ExpressionUUID->
  "461247b5-a29d-4511-bf39-21de21c4d73f"],
