Cell[TextData[{
 "When ",
 Cell[BoxData[
  ButtonBox["Fuse",
   BaseStyle->"Link",
   ButtonData->"paclet:Fusica/ref/Fuse"]], "InlineFormula",ExpressionUUID->
  "c01b1feb-2368-4076-a943-e7591e884cae"],
